<div id="errors">
<% if @preference.errors.any? %>
<ul>
  <% @preference.errors.full_messages.each do |msg| %>
    <li><%= msg %></li>
  <% end %>
</ul>
<% end %>
</div>
<%= form_for @preference do |f| %>
<div class="form-group">
  <%= f.label :name %>
  <%= f.text_field :name,	class:"form-control" %>
</div>
  <%= f.submit submit_text, class:"btn	btn-success" %>
<% end %>
